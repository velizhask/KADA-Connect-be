# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key

# Google Drive API Configuration
GOOGLE_PROJECT_ID=your-google-project-id
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----\n"

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
# Development origins (default)
ALLOWED_ORIGINS=http://localhost:3001,http://localhost:5173,https://yourfrontend.com

# API Configuration - Support multiple endpoints (comma-separated)
# Development endpoints (default)
API_BASE_URL=http://localhost:3001,https://yourbackend.com

